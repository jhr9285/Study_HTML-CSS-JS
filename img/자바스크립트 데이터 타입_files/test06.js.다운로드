var num; /* 변수 선언만 함. */
var obj = null; /* null이라는 데이터도 객체로 간주됨. */
/* document.getElementById('txt').innerHTML = '오늘 추워요!'; */
/* document.getElementById('txt').innerHTML = '어지러워요!'; */

document.getElementById('txt').innerHTML = '100 type : ' + typeof 100 + '<br>';
document.getElementById('txt').innerHTML = document.getElementById('txt').innerHTML + '3.14 type : ' + typeof 3.14 + '<br>';
document.getElementById('txt').innerHTML = document.getElementById('txt').innerHTML + '"dooly" type : ' + typeof 'dooly' + '<br>';
document.getElementById('txt').innerHTML = document.getElementById('txt').innerHTML + 'true type : ' + typeof true + '<br>';
document.getElementById('txt').innerHTML = document.getElementById('txt').innerHTML + '[1, 2, 3] type : ' + typeof [1, 2, 3] + '<br>';
document.getElementById('txt').innerHTML = document.getElementById('txt').innerHTML + '{name: "루피", job: "해적"} type : ' + typeof {name: "루피", job: "해적"} + '<br>';
document.getElementById('txt').innerHTML = document.getElementById('txt').innerHTML + 'num type : ' + typeof num + '<br>';
document.getElementById('txt').innerHTML = document.getElementById('txt').innerHTML + 'obj type : ' + typeof obj + '<br>';

console.log(document.getElementById('txt').innerHTML = document.getElementById('txt').innerHTML + 'obj type : ' + typeof obj + '<br>');
/* innerHTML : HTML 문서에 뒤쪽 내용을 써준다. 내용물을 기억하는 변수. 자바스크립트에서는 부르는 명칭이 '속성'이지만 쓰임새는 변수임. */
/* typeof : 뒤에 있는 데이터가 어떤 타입인지 알려주는 연산자. (number, string 등의 결과값이 나옴.) */
/* 두 줄 모두 띄우려면? '누적'시켜준다. document~부분을 추가 입력. */
/* console.log 로 함수 넣으면 그 함수 값을 크롬 개발자도구 console 창에서 볼 수 있다. */